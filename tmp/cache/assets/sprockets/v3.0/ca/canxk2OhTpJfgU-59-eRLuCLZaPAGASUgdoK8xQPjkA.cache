I"(function() {
  window.Views || (window.Views = {});

  Views.ApplicationView = (function() {
    var _set_up_elements;

    function ApplicationView() {
      _set_up_elements();
    }

    ApplicationView.prototype.setTab = function(number, total) {
      var title;
      if (total == null) {
        total = false;
      }
      if (total > 0) {
        number = total + number;
      }
      title = $('title').text();
      if (new RegExp(/\(\d\) (.*)/).test(title)) {
        if (number <= 0) {
          return $('title').text(title.replace(new RegExp(/\(\d\) (.*)/), '$1'));
        } else {
          return $('title').text(title.replace(new RegExp(/\(\d\) (.*)/), '(' + number + ') $1'));
        }
      } else {
        if (number > 0) {
          return $('title').prepend('(' + number + ') ');
        }
      }
    };

    _set_up_elements = function() {
      return $("#main_setting_icon").on('click', function() {
        return $('.ui.sidebar').sidebar('toggle');
      });
    };

    return ApplicationView;

  })();

}).call(this);
